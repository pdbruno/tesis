    "from qiskit import QuantumCircuit\n",
    "from qiskit.primitives import StatevectorSampler\n",
    "\n",
    "qc = QuantumCircuit(2)\n",
    "qc.h(1)\n",
    "qc.cx(1, 0)\n",
    "qc.measure_all()\n",
    "\n",
    "sampler = StatevectorSampler(default_shots=10000)\n",
    "sampler.run([qc]).result()[0].data['meas'].get_counts()"
   ]
